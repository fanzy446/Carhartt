@CHARSET "UTF-8";

.main_pane {
	width: 1000px;
	/* height: 600px; */
	/*background-color: silver;*/
	margin: 0 auto;
	/* background-color: yellow; */
}

.nav {
	height: 40px;
	width: 1000px;
	font-size: 16px;
	font-family: "微软雅黑";
}

.nav a:LINK {
	color: #666666;
}

.nav a:HOVER {
	text-decoration: none;
	color: blue;
}

.left_pane {
	width: 160px;
	float: left;
	/*height: 500px;*/
}

.middle_pane {
	width: 596px;
	/*height: 500px;*/
	background-color: #FAFAF8;
	float: left;
	border-left: 9px solid white;
	border-right: 10px solid white;
}

.right_pane {
	width: 225px;
	/*height: 500px;*/
	background-color: #FAFAF8;
	float: left;
}

/*--------------左边----------------*/
.head {
	width: 160px;
	height: 170px;
	background-color: #FAFAF8;
	float: left;
	border-bottom: 6px solid white;
}

.head_in {
	width: 130px;
	height: 170px;
	float: left;
	text-align: center;
	margin: 10px 15px 10px 15px;
}

.head_in img {
	width: 128px;
	border: 1px solid silver;
}

.head_in p {
	margin-top: 5px;
	margin-bottom: 5px;
	font-size: 16px;
	font-family: "微软雅黑";
}

.faceul {
	width: 160px;
	float: left;
}

.faceul li {
	border-top: 3px solid white;
	background-color: #F6FBFA;
	text-align: center;
	height: 45px;
	font-size: 16px;
	font-family: "微软雅黑";
}

.faceul a {
	display: block;
	height: 45px;
	background-color: #F2F1D7;
}

.faceul a:HOVER {
	text-decoration: none;
	background: #ECF8F3;
	color: #676767;
}

.faceul span {
	margin-top: 12px;
	display: block;
	float: left;
	margin-left: 48px;
}

/*---------------------右边---------------------*/
.announcement {
	width: 225px;
	/*height: 166px;*/
	float: left;
}

.help {
	width: 225px;
	/*height: 166px;*/
	float: left;
	border-top: 9px solid white;
}

.code2d {
	width: 225px;
	/*height: 166px;*/
	float: left;
	border-top: 9px solid white;
}

.title {
	width: 225px;
	height: 34px;
	background-color: #F2F1D7;
	float: left;
}

.title span {
	margin-top: 5px;
	margin-left: 5px;
	float: left;
	font-size: 16px;
	font-family: "微软雅黑";
}

.title a {
	float: right;
	margin-top: 5px;
	font-size: 16px;
	font-family: "微软雅黑";
}

.content {
	width: 225px;
	/* height: 135px; */
	float: left;
}

.content li {
	font-size: 14px;
	display: block;
	border: 5px solid #FAFAF8;
	height:20px; 
	font-family: "微软雅黑";

}
.content .content_img {
	width: 145px;
	 margin-left: 40px; 
	margin-top: 5px;
	display: block;
}