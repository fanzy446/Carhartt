@CHARSET "UTF-8";

.feedback_main {
	width: 596px;
}

.feedback_top_ad {
	width: 596px;
}

.feedback_top_ad img {
	width: 596px;
}

.feed_det_pane {
	width: 596px;
}

.feed_det_pane table {
	width: 576;
	margin-top: 10px;
	margin-left: 10px;
}

.feed_det_pane .head_tr {
	height: 40px;
	background-color: #F2F1D7;
	margin-top: 10px;
	margin-bottom: 10px;
	
}
.head_tr span{
	font-size: 16px;
	font-family: "微软雅黑";
	color: #66666F;
}
.feed_det_pane .body_tr {
	display: block;
	margin-top: 10px;
	margin-bottom: 10px;
	background-color: #FAFAF8;
	line-height: 150%;
	text-align: justify;
}

.feed_det_pane .foot_tr {
	line-height: 150%;
	background-color: #F2F1D7;
	margin-top: 10px;
	margin-bottom: 10px;
	text-align: justify;
	height: 50px;
}

.foot_tr span{
	font-size: 16px;
	font-family: "微软雅黑";
	color: #666666;
}
.body_tr span{
	font-size: 15px;
	font-family: "微软雅黑";
	color: #666666;
}
.feedback_ad {
	width: 596px;
}

.feedback_ad img {
	width: 596px;
}

.ok {
	width: 596px;
	height: 30px;
	background-color: #FAFAF8;
	float: left;
	border-bottom: 5px solid white;
}

/*------bootstrap样式------*/
.table-hover tbody tr:hover>td,.table-hover tbody tr:hover>th {
	background-color: #f5f5f5;
	color: blue;
}

.table-hover tbody tr.success:hover>td {
	background-color: #d0e9c6;
}

.table-hover tbody tr.error:hover>td {
	background-color: #ebcccc;
}

.table-hover tbody tr.warning:hover>td {
	background-color: #faf2cc;
}

.table-hover tbody tr.info:hover>td {
	background-color: #c4e3f3;
}
/*------bootstrap样式------*/
.btn {
	width: 100px;
	height: 30px;
	background-color: #EAF8FF;
	border: 0;
	cursor: pointer;
	font-size: 15px;
}