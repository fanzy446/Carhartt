@CHARSET "UTF-8";

.addr_main {
	width: 596px;
}

.addr_top {
	width: 596px;
}

.addr_top img {
	width: 596px;
}

.hello {
	height: 40px;
	width: 596px;
	font-family: "微软雅黑";
	font-size: 16px;
	border-bottom: 3px dotted #CFCDC0;
}

.hello span {
	font-weight: bold;
	width: 400px;
	display: block;
	float: left;
	margin-left: 18px;
	margin-top: 18px;
}

.hello a {
	float: right;
	margin-right: 10px;
	display: block;
	margin-top: 18px;
}

.addr_pane {
	width: 596px;
}

.addr_list {
	width: 596px;
}

.addr_list table {
	width: 100%;
}

.addr_list span {
	font-size: 15px;
	font-family: "微软雅黑";
	color: #66666F;
}

.addr_list tr {
	height: 40px;
}

.new_td {
	border-top: 3px dotted #D0CEC1;
}

.addr_new {
	width: 576px;
	display: none;
	margin-left: 20px;
}

.addr_new table {
	width: 100%;
	height: 200px;
	margin-top: 10px;
}

.addr_new span {
	display: block;
	text-align: center;
	font-size: 15px;
	font-family: "微软雅黑";
	color: #666666;
}

.addr_new tr {
	height: 40px;
}

.addr_new .new_input {
	height: 90%;
	width: 300px;
}

.addr_ad {
	width: 596px;
}

.addr_ad img {
	width: 596px;
}

.ok {
	width: 596px;
	height: 30px;
	background-color: #FAFAF8;
	float: left;
	border-bottom: 5px solid white;
}

/*------bootstrap样式------*/
.table-hover tbody tr:hover>td,.table-hover tbody tr:hover>th {
	background-color: #f5f5f5;
	color: blue;
}

.table-hover tbody tr.success:hover>td {
	background-color: #d0e9c6;
}

.table-hover tbody tr.error:hover>td {
	background-color: #ebcccc;
}

.table-hover tbody tr.warning:hover>td {
	background-color: #faf2cc;
}

.table-hover tbody tr.info:hover>td {
	background-color: #c4e3f3;
}
/*------bootstrap样式------*/
.btn { /* width: 100px;
	height: 30px;
	background-color: #EAF8FF;
	border: 0;
	cursor: pointer;
	font-size: 15px; */
	width: 100px;
	height: 30px;
	background-color: #F2F1D7;
	border: 0;
	cursor: pointer;
	font-size: 15px;
	font-family: "微软雅黑";
	color: #666666;
}

.btn:HOVER {
	background-color: #ECF8F3;
}

#norecord {
	width: 596px;
	height: 50px;
	display: none;
}

#norecord span {
	display: block;
	margin-top: 10px;
	font-size: 15px;
	font-family: "微软雅黑";
	float: left;
	margin-left: 270px;
	color: #666666;
}